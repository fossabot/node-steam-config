{
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": [
          "jsdoc", "closure"
        ]
    },
    "source": {
        "include": [
          "./package.json",
          "./README.md",
          "./src/index.js",
          "./src/steamPaths.js",
          "./src/steamUtils.js"
        ],
        "excludePattern": "(asyncLib|bvdf|cleanSteam)"
    },
    "plugins": [
      "plugins/markdown"
    ],
    "templates": {
        "footer": "",
        "referenceTitle": "SteamConfig API",
        "disableSort": true,
        "navType": "inline",
        "inverseNav": true,
        "monospaceLinks": true,
        "includeDate": false,
        "collapseSymbols": true,
        "outputSourceFiles": false,
        "outputSourcePath": false,
        "sort": false
    },
    "opts": {
      "template": "./node_modules/ink-docstrap/template",
      "destination": "./docs/",
      "recurse": false
    }
}
