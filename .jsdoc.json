{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "sourceType": "module",
    "source": {
        "include": [
          "./package.json",
          "./README.md",
          "./lib/index.js",
          "./lib/steam-paths.js",
          "./lib/steamdata-utils.js",
          "./lib/steam-settings.js"
        ]
    },
    "plugins": [
      "plugins/markdown"
    ],
    "templates": {
        "referenceTitle": "SteamConfig API Reference",
        "disableSort": true,
        "monospaceLinks": true,
        "default": {
          "includeDate": false,
          "outputSourceFiles": false
        }
    },
    "opts": {
      "template": "./jsdoc-template-master",
      "destination": "./docs/",
      "encoding": "utf8",
      "recurse": false
    }
}
